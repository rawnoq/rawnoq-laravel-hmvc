{
    "name": "rawnoq/laravel-hmvc",
    "version": "1.0.9",
    "description": "A professional Laravel package for Hierarchical Model-View-Controller (HMVC) architecture with complete modular structure support",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "hmvc",
        "modular",
        "modules",
        "architecture",
        "mvc",
        "structure"
    ],
    "authors": [
        {
            "name": "Rawnoq"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rawnoq\\HMVC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawnoq\\HMVC\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawnoq\\HMVC\\Providers\\HMVCServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

